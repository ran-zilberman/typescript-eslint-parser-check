{
  "parser": "typescript-eslint-parser",
  "env": {
    "jquery": true,
    "prototypejs": true
  },
  "globals": {
    "_": false,
    "angular": false,
    "require": false,
    "module": false
  },
  "rules": {
    "no-bitwise": 2,
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": 2,
    "new-cap": 2,
    "no-caller": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": 0,
    "one-var": 0,
    "no-underscore-dangle": 0,
    "key-spacing": [
      2,
      {
        "afterColon": true
      }
    ],
    "var-on-top": 0
  }
}
